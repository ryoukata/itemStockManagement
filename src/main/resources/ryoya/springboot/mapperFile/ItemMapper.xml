<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ryoya.springboot.mapperFile.ItemMapper">
	<select id="findStartItems" resultType="ryoya.springboot.domain.Item">
		SELECT
			id,
			name,
			condition,
			category,
			brand,
			price,
			shipping,
			description
		FROM
			items
		LIMIT 
			100
	</select>
</mapper>